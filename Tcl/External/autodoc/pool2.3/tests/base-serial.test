if {[string compare test [info procs test]] == 1} then {source defs}


test base-serial-1.0 {serial::new/delete/new, initial} {

    set x [::pool::serial::new]
    ::pool::serial::delete $x
    set y [::pool::serial::new]

    set ok [expr {$x == $y}]
} {1}


catch {unset x y}

