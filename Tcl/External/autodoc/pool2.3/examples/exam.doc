    defterm ocDialog     "[classRef okCancelDialog]"
    defterm gMenubar     "[classRef genericMenubar]"
    defterm gPop         "[classRef genericPopupMenu]"
    defterm iDialog      "[classRef infoDialog]"
    defterm logger       "[classRef logger]"
}

